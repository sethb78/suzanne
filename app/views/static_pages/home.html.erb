<!-- Video -->
<div class="row">
  <div class="col-md-12 col-xs-12">
    <iframe width="560" height="315" src="//www.youtube.com/embed/-pk_5uAsumY?rel=0" frameborder="0" allowfullscreen></iframe>
  </div>
</div>

<!-- Blog -->
<div class="row blog">
  <div class="col-md-12 col-xs-12 ">
    <h3 class="text-center"><strong><%= @current_blog.title%></strong></h3>
    <h5 class="text-center"><%= @current_blog.updated_at.strftime('%m/%d/%Y at %I:%M%p')  %></h5>
    <%= simple_format(@current_blog.content) %>
  </div>
</div>
